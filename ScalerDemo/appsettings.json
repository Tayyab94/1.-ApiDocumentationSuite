{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "JwtSettings": {
    "Issuer": "https://your-auth-provider.com", // Replace with your auth server URL
    "Audience": "your-api-audience", // Replace with your API audience
    "SecretKey": "your-secret-key" // Replace with a strong secret key (for testing)
  } 
}
